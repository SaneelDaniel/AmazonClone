product\">\n\n            <div className=\"product__info\">\n                <p>{title}</p>\n                <p className=\"product__price\">\n                    <small>$</small>\n                    <strong>{price}</strong>\n                </p>\n\n                <div className=\"product__rating\">\n                    {Array(rating)\n                    .fill()\n                    .map((_, i) =>(\n                        <p>ðŸŒŸ</p>\n                    ))}\n                </div>\n            </div>\n            \n            <img\n                src={image} alt=\"\"\n            />\n            \n            \n\n            <button onClick={addToBasket} >Add to basket</button>\n        </div>\n    )\n}\n\nexport default Product\n","/Users/danielsaneel/Desktop/React-Native Environment/React Ecom Temp/ecommerce-template/src/Checkout.js",["105"],"import React from 'react'\nimport \"./Checkout.css\"\nimport CheckoutProduct from './CheckoutProduct';\nimport { useStateValue } from './StateProvider';\nimport Subtotal from './Subtotal'\n\nfunction Checkout() {\n\n    const [{basket, user}, dispatch] = useStateValue();\n    \n    return (\n        <div className =\"checkout\">\n\n            <div className=\"checkout__left\">\n                <img\n                    className=\"checkout__ad\"\n                    src=\"https://www.karooya.com/blog/wp-content/uploads/2018/12/AmazonAdvertising.png\"\n                    alt=\"\"\n                />\n                <div>\n                    <h3>Hello {user?.email}</h3>\n                    <h2 className=\"checkout__title\">Your Shopping Basket</h2>\n\n                    {basket.map (item => (\n                        <CheckoutProduct\n                            id={item.id}\n                            title={item.title}\n                            image={item.image}\n                            price={item.price}\n                            rating={item.rating}\n                        />\n                    ))}\n\n                    {/* CheckoutProduct */}\n                    {/* CheckoutProduct */}\n                    {/* CheckoutProduct */}\n                </div>\n            </div>\n\n            <div className=\"checkout__right\">\n                <Subtotal />\n            </div>\n            \n        </div>\n    )\n}\n\nexport default Checkout\n","/Users/danielsaneel/Desktop/React-Native Environment/React Ecom Temp/ecommerce-template/src/Subtotal.js",["106"],"import React from 'react'\nimport \"./Subtotal.css\"\nimport CurrencyFormat from \"react-currency-format\"\nimport { useStateValue } from './StateProvider';\nimport { getBasketTotal } from './reducer';\nimport { useHistory } from 'react-router-dom';\n\nfunction Subtotal() {\n    const [{basket}, dispatch] = useStateValue();\n    const history = useHistory();\n    \n    return (\n        <div className=\"subtotal\">\n            <CurrencyFormat\n                renderText={(value) =>(\n                    <>\n                        <p>\n                            Subtotal ({basket?.length} items): <strong>{value}</strong>\n                        </p>\n                        <small className=\"subtotal__gift\">\n                            <input type=\"checkbox\"/> This order contais a gift\n                        </small>\n                    </>\n                )}\n                decimalScale={2}\n                value={getBasketTotal(basket)}\n                displayType={\"text\"}\n                thousandSeperator={true}\n                prefix={\"$\"}\n            />\n            <button onClick = {e => history.push('/payment')}>Proceed to Checkout</button>\n        </div>\n    )\n}\n\nexport default Subtotal\n","/Users/danielsaneel/Desktop/React-Native Environment/React Ecom Temp/ecommerce-template/src/reportWebVitals.js",[],"/Users/danielsaneel/Desktop/React-Native Environment/React Ecom Temp/ecommerce-template/src/index.js",[],["107","108"],"/Users/danielsaneel/Desktop/React-Native Environm